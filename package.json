{
  "name": "@windingtree/docs",
  "version": "0.0.0",
  "description": "Winding Tree developer documentation",
  "main": "README.md",
  "scripts": {
    "build-oas-views": "node management/prepare-oas-views.js",
    "build-datamodel-views": "node management/prepare-datamodel-views.js",
    "copy-legacy-swagger-ui": "rimraf -r book/swagger-ui/ && cp -r swagger-ui book/",
    "copy-docson": "rimraf -r book/assets/docson/* && cp -r node_modules/docson/public/* book/assets/docson/",
    "build-gitbook": "gitbook build",
    "clean": "rimraf _book",
    "build": "npm run clean && ./management/build.sh",
    "dev": "npm run clean && gitbook serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windingtree/docs.git"
  },
  "keywords": [
    "winding",
    "tree",
    "ethereum"
  ],
  "author": "Winding Tree Developers <dev@windingtree.com>",
  "license": "CC-BY-SA-3.0",
  "bugs": {
    "url": "https://github.com/windingtree/docs/issues"
  },
  "homepage": "https://github.com/windingtree/docs#readme",
  "devDependencies": {
    "docson": "^2.1.0",
    "gitbook-cli": "^2.3.2",
    "gitbook-plugin-callouts": "^0.1.1",
    "gitbook-plugin-insert-logo-link": "^1.0.1",
    "gitbook-plugin-last-modified": "^1.0.0",
    "rimraf": "^2.6.3",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "wt-booking-api": "git+https://git@github.com/windingtree/wt-booking-api.git#v0.3.1",
    "wt-notification-api": "git+https://git@github.com/windingtree/wt-notification-api.git#v0.3.0",
    "wt-read-api": "git+https://git@github.com/windingtree/wt-read-api.git#v0.10.0",
    "wt-search-api": "git+https://git@github.com/windingtree/wt-search-api.git#v0.3.0",
    "wt-write-api": "git+https://git@github.com/windingtree/wt-write-api.git#v0.11.0"
  }
}
